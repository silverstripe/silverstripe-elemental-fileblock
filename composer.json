{
    "name": "silverstripe/elemental-fileblock",
    "description": "A file block for the silverstripe-elemental module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "4.13.0-beta1",
        "silverstripe/vendor-plugin": "^1.0",
        "dnadesign/silverstripe-elemental": "4.11.0-beta1",
        "silverstripe/cms": "4.13.0-beta1",
        "silverstripe/assets": "1.13.0-beta1"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^2"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ElementalFileBlock\\": "src/",
            "SilverStripe\\ElementalFileBlock\\Tests\\": "tests/",
            "SilverStripe\\ElementalFileBlock\\Tests\\Behat\\Context\\": "tests/behat/src/"
        }
    }
}